{
  "host": "0.0.0.0",
  "port": 9999,
  "routes": {
    "/hello": {
      "method": "GET",
      "params": ["message"],
      "commands": [
        {
          "condition": "{message} = hello",
          "command": "echo {message}"
        }
      ]
    },
    "/date": {
      "method": "GET",
      "commands": [
        {
          "command": "date /t"
        }
      ]
    }
  },
  "default_responses": {
    "404": {
      "type": "string",
      "text": "This path was not found :)"
    }
  }
}